{
    "name": "inquirer-fuzzy-path",
    "version": "2.3.0",
    "description": "Fuzzy file/directory search and select prompt for inquirer.",
    "license": "MIT",
    "homepage": "https://github.com/adelsz/inquirer-fuzzy-path",
    "authors": [
        "adelsz",
        "Clay Dunston <dunstontc@gmail.com>"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/tcd/inquirer-fuzzy-path.git"
    },
    "engines": {
        "node": ">=8.0"
    },
    "main": "lib/index.cjs",
    "scripts": {
        "build": "npm run build:rollup",
        "build:rollup": "cross-env NODE_ENV=production rollup -c rollup.config.js",
        "build:types": "cross-env tsc --project ./tsconfig.json --declaration --emitDeclarationOnly"
    },
    "dependencies": {
        "ansi-styles": "^6.1.0",
        "fuzzy": "^0.1.3",
        "inquirer": "^8.0.0",
